{
  "name": "keq-debug",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "ava",
    "test:debug": "ava --verbose",
    "test:update": "ava -u",
    "report": "nyc --reporter=html --reporter=text-summary --skip-full ava",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "ci": "git-cz",
    "ca": "git add -A && git-cz -av",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release",
    "prepublishOnly": "npm test && npm run build && pinst --disable",
    "postpublish": "pinst --enable && git push",
    "clean": "rm -rf ./lib/*",
    "prebuild": "npm run clean",
    "build:es": "ttsc -p build/tsconfig.es.json && npm run copyfile:es:js && npm run copyfile:es:dts",
    "build:lib": "ttsc -p build/tsconfig.lib.json && npm run copyfile:lib:js && npm run copyfile:lib:dts",
    "build": "npm run build:lib && npm run build:es",
    "watch": "NODE_ENV=development ttsc -p build/tsconfig.lib.json -w",
    "copyfile:es:js": "copyfiles -e \"@(build|es|lib)/**/*.js\" **/*.js es",
    "copyfile:es:dts": "copyfiles -e \"@(build|es|lib)/**/*.d.ts\" **/*.d.ts es",
    "copyfile:lib:js": "copyfiles -e \"@(build|es|lib)/**/*.js\" **/*.js lib",
    "copyfile:lib:dts": "copyfiles -e \"@(build|es|lib)/**/*.d.ts\" **/*.d.ts lib",
    "postinstall": "is-ci || husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "ava": "^3.10.1",
    "commitizen": "^4.2.3",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.20.0",
    "husky": "^5.1.1",
    "is-ci": "^2.0.0",
    "keq": "^1.1.2",
    "lint-staged": "^10.5.4",
    "mili": "^3.9.3",
    "nyc": "^15.1.0",
    "pinst": "^2.1.6",
    "sinon": "^9.2.4",
    "standard-version": "^9.1.1",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.2",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "@types/node": "^14.14.31",
    "object-inspect": "^1.10.2",
    "ramda": "^0.27.1",
    "url": "^0.11.0"
  },
  "description": "[![version](https://img.shields.io/npm/v/keq-debug.svg?style=flat-square)](https://www.npmjs.com/package/keq-debug) [![downloads](https://img.shields.io/npm/dm/keq-debug.svg?style=flat-square)](https://www.npmjs.com/package/keq-debug) [![license](https://img.shields.io/npm/l/keq-debug.svg?style=flat-square)](https://www.npmjs.com/package/keq-debug) [![dependencies](https://img.shields.io/david/Val-istar-Guo/keq-debug.svg?style=flat-square)](https://www.npmjs.com/package/keq-debug) [![coveralls](https://img.shields.io/coveralls/github/Val-istar-Guo/keq-debug.svg?style=flat-square)](https://coveralls.io/github/Val-istar-Guo/keq-debug)",
  "version": "1.0.1",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Val-istar-Guo/keq-debug.git"
  },
  "keywords": [
    "keq",
    "debug"
  ],
  "author": "Val.istar.Guo <val.istar.guo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Val-istar-Guo/keq-debug/issues"
  },
  "homepage": "https://github.com/Val-istar-Guo/keq-debug#readme"
}
